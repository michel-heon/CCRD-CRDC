crdc-ccrd-data:RDS101
        a                          skos:Concept , owl:Class , crdc-ccrd:CRDC_CCRD_Division ;
        rdfs:label                 "Exploration and exploitation of the earth"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 1" ;
        rdfs:subClassOf            crdc-ccrd:SEO_OSE_Entity ;
        crdc-ccrd:classDefinition  "This division is a socioeconomic objective which covers research and experimental development (R&D) activities with objectives related to the exploration of the Earth's crust and mantle, seas, oceans and atmosphere, as well as for R&D activities on their exploitation. It also includes climatic and meteorological research, polar exploration and hydrology. It does not include R&D activities related to soil improvement (objective 4 or RDS104), land use or fishing (objective 8 or RDS108), or pollution (objective 2 or RDS102)."@en-ca ;
        crdc-ccrd:classTitle       "Exploration and exploitation of the earth"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Division"@en-ca ;
        crdc-ccrd:hasID            "RDS101" ;
        crdc-ccrd:hasLevel         "1"^^xsd:double .

