        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards land associated with water management in various natural environments and ecosystems. It includes urban and industrial land and water management issues, and the allocation of water for agricultural production."@en-ca ;
        crdc-ccrd:classTitle       "Land and water management"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS10407" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
