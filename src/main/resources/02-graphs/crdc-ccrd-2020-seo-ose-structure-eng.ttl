crdc-ccrd-data:RDS10205
        a                          owl:Class , crdc-ccrd:CRDC_CCRD_Group , skos:Concept ;
        rdfs:label                 "Control and care of the environment"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 13" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS102 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards assessment and management of various ecosystems; environmental and natural resources; and, finding appropriate ways to nurture and rehabilitate seriously degraded environments back to original condition or to an improved state. This group also covers R&D directed towards an understanding of natural hazards (such as bushfire, cyclones and storms, earthquakes, landslides, tsunamis and volcanic eruptions), their impact on lives, property and environments and the development of warning systems and control measures to minimise impacts from these hazards."@en-ca ;
        crdc-ccrd:classTitle       "Control and care of the environment"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS10205" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS102 .

