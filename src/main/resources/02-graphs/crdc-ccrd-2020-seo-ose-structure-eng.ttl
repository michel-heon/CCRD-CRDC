crdc-ccrd-data:RDS10501
        a                          owl:Class , crdc-ccrd:CRDC_CCRD_Group , skos:Concept ;
        rdfs:label                 "Mining and extraction of energy resources"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 32" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS105 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards mining and extracting energy mineral resources, as well as the extraction of geothermal energy."@en-ca ;
        crdc-ccrd:classTitle       "Mining and extraction of energy resources"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS10501" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS105 .

crdc-ccrd-data:RDS10806
        a                          owl:Class , skos:Concept , crdc-ccrd:CRDC_CCRD_Group ;
        rdfs:label                 "Livestock raising (including beekeeping and pet breeding)"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 64" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS108 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards farming or breeding livestock. It includes beekeeping and pet breeding."@en-ca ;
        crdc-ccrd:classTitle       "Livestock raising (including beekeeping and pet breeding)"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS10806" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS108 .

crdc-ccrd-data:RDS11007
        a                          crdc-ccrd:CRDC_CCRD_Group , skos:Concept , owl:Class ;
        rdfs:label                 "Heritage (including Indigenous culture)"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 79" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS110 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards heritage issues, including the conservation of natural, social and cultural heritage. This group also covers R&D directed towards indigenous culture, knowledge, language, and rights."@en-ca ;
        crdc-ccrd:classTitle       "Heritage (including Indigenous culture)"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS11007" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS110 .

crdc-ccrd-data:RDS11101
        a                          skos:Concept , owl:Class , crdc-ccrd:CRDC_CCRD_Group ;
        rdfs:label                 "Finance and economics"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 85" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS111 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards the development and provision of finance and investment services. It includes but is not limited to banks, building societies, credit unions and money market dealing; investment and superannuation services; and insurance services. This group also covers R&D directed towards the understanding of economic issues at the microeconomic and macroeconomic level. It includes effects on taxation, consumption, industrial organisations, industry policy, industry cost and structure, fiscal policy, monetary policy, exchange rates, balance of payments, income policy, and taxation."@en-ca ;
        crdc-ccrd:classTitle       "Finance and economics"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS11101" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS111 .

crdc-ccrd-data:RDS10401
        a                          crdc-ccrd:CRDC_CCRD_Group , skos:Concept , owl:Class ;
        rdfs:label                 "Construction planning"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 23" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS104 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards improving the activities associated with the development of construction projects to the sketch plan stage."@en-ca ;
        crdc-ccrd:classTitle       "Construction planning"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS10401" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS104 .

crdc-ccrd-data:RDS111
        a                          owl:Class , crdc-ccrd:CRDC_CCRD_Division , skos:Concept ;
        rdfs:label                 "Political and social systems, structures and processes"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 84" ;
        rdfs:subClassOf            crdc-ccrd:SEO_OSE_Entity ;
        crdc-ccrd:classDefinition  "This division is a socioeconomic objective which covers research and experimental development (R&D) activities with objectives to improve the understanding of and support the political structure of society; public administration issues and economic policy; regional studies and multi-level governance; social change, social processes and social conflicts; the development of social security and social assistance systems; and the social aspects of the organisation of work. This objective also includes de R&D activities related to gender-related social studies, including discrimination and familiar problems; the development of methods of combating poverty at local, national and international levels; the protection of specific population categories on the social level (immigrants, delinquents, \"drop outs\", etc.), on the sociological level, i.e. with regard to their way of life (young people, adults, retired people, disabled people, etc.) and on the economic level (consumers, farmers, fishermen, miners, the unemployed, etc.); and methods of providing social assistance when sudden changes (natural, technological or social) occur in society. This objective does not include R&D activities related to industrial health, the health control of communities from the organisational and socio-medical point of view, pollution at the place of work, the prevention of industrial accidents and the medical aspects of the causes of industrial accidents (objective 7 or RDS107)."@en-ca ;
        crdc-ccrd:classTitle       "Political and social systems, structures and processes"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Division"@en-ca ;
        crdc-ccrd:hasID            "RDS111" ;
        crdc-ccrd:hasLevel         "1"^^xsd:double .

crdc-ccrd-data:RDS11001
        a                          skos:Concept , owl:Class , crdc-ccrd:CRDC_CCRD_Group ;
        rdfs:label                 "Information services"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 73" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS110 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards information services. It includes but is not limited to libraries, library databases and information processing; presentation of information, including displays, in libraries, museums and art galleries, educational centres and similar facilities; and electronic archives, open access, and news collection services."@en-ca ;
        crdc-ccrd:classTitle       "Information services"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS11001" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS110 .

crdc-ccrd-data:RDS10301
        a                          owl:Class , skos:Concept , crdc-ccrd:CRDC_CCRD_Group ;
        rdfs:label                 "Scientific exploration of space"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 16" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS103 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards improving the scientific exploration of space, including robotic exploration of the solar system, and advanced exploration development."@en-ca ;
        crdc-ccrd:classTitle       "Scientific exploration of space"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS10301" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS103 .

crdc-ccrd-data:RDS10606
        a                          owl:Class , crdc-ccrd:CRDC_CCRD_Group , skos:Concept ;
        rdfs:label                 "Veterinary pharmaceutical products"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 45" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS106 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards the manufacture of pharmaceutical products used in the prevention, diagnosis and treatment of veterinary diseases."@en-ca ;
        crdc-ccrd:classTitle       "Veterinary pharmaceutical products"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS10606" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS106 .

crdc-ccrd-data:RDS109
        a                          crdc-ccrd:CRDC_CCRD_Division , owl:Class , skos:Concept ;
        rdfs:label                 "Education"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 67" ;
        rdfs:subClassOf            crdc-ccrd:SEO_OSE_Entity ;
        crdc-ccrd:classDefinition  "This division is a socioeconomic objective which covers research and experimental development (R&D) activities with objectives related to supporting general or special education, including training, pedagogy, didactics, and targeted methods for specially gifted persons or those with learning disabilities. This objective applies to all levels of education, from pre- and primary school through to tertiary education, as well as to subsidiary services to education."@en-ca ;
        crdc-ccrd:classTitle       "Education"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Division"@en-ca ;
        crdc-ccrd:hasID            "RDS109" ;
        crdc-ccrd:hasLevel         "1"^^xsd:double .

crdc-ccrd-data:RDS10201
        a                          owl:Class , crdc-ccrd:CRDC_CCRD_Group , skos:Concept ;
        rdfs:label                 "Environmentally sustainable human activities"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 9" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS102 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards environmentally sustainable production of plants and plant primary products; animals and animal primary products; exploration, mining, extraction, and processing of mineral resources; manufactured materials; construction and building activities; transport activities; and, information and communication service activities by minimising the generation of emissions and waste and by reducing or optimising the consumption of water and other natural resources."@en-ca ;
        crdc-ccrd:classTitle       "Environmentally sustainable human activities"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS10201" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS102 .

crdc-ccrd-data:RDS10506
        a                          owl:Class , skos:Concept , crdc-ccrd:CRDC_CCRD_Group ;
        rdfs:label                 "Energy conservation and efficiency"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 37" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS105 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards the conservation and efficient use of energy in industry, transport, commerce and households. It includes energy used in forms such as petroleum products, electricity, gas, coke and briquettes and in the form of heat energy."@en-ca ;
        crdc-ccrd:classTitle       "Energy conservation and efficiency"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS10506" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS105 .

<>      a                owl:Ontology ;
        rdfs:comment     "The Canadian Research and Development Classification (CRDC) was developed conjointly by the Social Sciences and Humanities Research Council of Canada (SSHRC), the Natural Sciences and Engineering Research Council of Canada (NSERC), the Canada Foundation for Innovation (CFI), the Canadian Institutes of Health Research (CIHR), and Statistics Canada which is the custodian. This shared standard classification, inspired by the Frascati Model 2015 of the Organisation for Economic Co-operation and Development (OECD), will be used by the federal granting agencies and Statistics Canada to collect and disseminate data related to research and development in Canada. The CRDC first official version is the 2020 version 1.0 and it is composed of 3 main pieces: the type of activity orTOA (with 3 categories), the field of research or FOR (with 1663 fields at the lowest level) and socioeconomic objective or SEO (with 85 main groups at the lowest level)."@en-ca , "La Classification canadienne de la recherche et du développement (CCRD) 2020 a été élaborée conjointement par le Conseil de recherches en sciences humaines du Canada (CRSH), le Conseil de recherches en sciences naturelles et en génie du Canada (CRSNG), la Fondation canadienne pour l'innovation (FCI), les Instituts de recherche en santé du Canada (IRSC) et Statistique Canada qui en est le gardien. Cette classification-type conjointe, inspirée du modèle de Frascati 2015 de l'Organisation de Coopération et de Développement Économiques (OCDE), sera utilisée par les organismes subventionnaires fédéraux et Statistique Canada pour recueillir et diffuser des données liées à la recherche et au développement au Canada. La première version officielle de la CCRD est la version 1.0 de 2020 qui est composée de 3 éléments principaux: le type d'activité ou TDA (avec 3 catégories), le domaine de recherche ou DDR (avec 1663 domaines au niveau le plus bas) et l'objectif socioéconomique ou OSE (avec 85 groupes principaux au niveau le plus bas)"@fr-ca ;
        rdfs:seeAlso     <https://www.oecd.org/sti/inno/frascati-manual.htm> , <https://www.statcan.gc.ca/eng/subjects/standard/crdc/2020v1/index> , <https://www.statcan.gc.ca/fra/sujets/norme/ccrd/2020v1/indice> ;
        owl:imports      <http://purl.org/uqam.ca/vocabulary/crdc-ccrd> ;
        owl:versionIRI   <http://purl.org/vivo.uqam.ca/crdc_ccrd/ontology#v.1.0.0-20210721> ;
        owl:versionInfo  "Based on CRDC/CCRD 2020 Version 1.0 (This standard was approved as a recommended standard on May 26, 2020. )" , "Author: Michel Héon PhD : heon.michel@uqam.ca" , "Transformed in ontology by Université du Québec à Montréal (2021/07/21)" .

crdc-ccrd-data:RDS10101
        a                          skos:Concept , crdc-ccrd:CRDC_CCRD_Group , owl:Class ;
        rdfs:label                 "Mineral exploration"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 2" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS101 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards exploration for minerals, other than energy mineral resources. It includes precious and semi-precious stones and mineral sands."@en-ca ;
        crdc-ccrd:classTitle       "Mineral exploration"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS10101" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS101 .

crdc-ccrd-data:RDS10406
        a                          owl:Class , crdc-ccrd:CRDC_CCRD_Group , skos:Concept ;
        rdfs:label                 "Communication technologies, systems and services"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 28" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS104 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards fixed line, mobile and satellite communication networks and services. It includes videoconference services."@en-ca ;
        crdc-ccrd:classTitle       "Communication technologies, systems and services"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS10406" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS104 .

crdc-ccrd-data:RDS11106
        a                          crdc-ccrd:CRDC_CCRD_Group , skos:Concept , owl:Class ;
        rdfs:label                 "Justice and the law"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 90" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS111 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards justice and the law. Not limited to but including legal processes; civil and criminal justice; crime prevention, law enforcement and correctional services; and, law reform."@en-ca ;
        crdc-ccrd:classTitle       "Justice and the law"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS11106" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS111 .

crdc-ccrd-data:RDS103
        a                          skos:Concept , crdc-ccrd:CRDC_CCRD_Division , owl:Class ;
        rdfs:label                 "Exploration and exploitation of space"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 15" ;
        rdfs:subClassOf            crdc-ccrd:SEO_OSE_Entity ;
        crdc-ccrd:classDefinition  "This division is a socioeconomic objective which covers all civil space research and experimental development (R&D) activities with objectives related to the R&D in exploration of space, space laboratories, space travel and launch systems. Although civil space R&D activities are not always concerned with particular objectives, they frequently have a specific goal, such as the advancement of knowledge (e.g. astronomy) or relate to particular applications (e.g. telecommunications satellites or earth observation). This objective does not include corresponding R&D activities for defence purposes."@en-ca ;
        crdc-ccrd:classTitle       "Exploration and exploitation of space"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Division"@en-ca ;
        crdc-ccrd:hasID            "RDS103" ;
        crdc-ccrd:hasLevel         "1"^^xsd:double .

crdc-ccrd-data:RDS11006
        a                          skos:Concept , owl:Class , crdc-ccrd:CRDC_CCRD_Group ;
        rdfs:label                 "Communication, literature and language"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 78" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS110 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards improving understanding of the social and cultural contexts of communication. It includes but is not limited to languages, literature, journalism, linguistics and semantics."@en-ca ;
        crdc-ccrd:classTitle       "Communication, literature and language"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS11006" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS110 .

crdc-ccrd-data:RDS10805
        a                          owl:Class , crdc-ccrd:CRDC_CCRD_Group , skos:Concept ;
        rdfs:label                 "Fisheries (including aquaculture and wild caught)"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 63" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS108 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards improving the characteristics and harvesting of fisheries grown or farmed in aquaculture, or caught in the wild, either for commercial, recreational or other reasons."@en-ca ;
        crdc-ccrd:classTitle       "Fisheries (including aquaculture and wild caught)"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS10805" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS108 .

crdc-ccrd-data:RDS110
        a                          crdc-ccrd:CRDC_CCRD_Division , owl:Class , skos:Concept ;
        rdfs:label                 "Culture, recreation, religion and media"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 72" ;
        rdfs:subClassOf            crdc-ccrd:SEO_OSE_Entity ;
        crdc-ccrd:classDefinition  "This division is a socioeconomic objective which covers research and experimental development (R&D) activities with objectives to improve the understanding of social phenomena related to cultural activities, religion and leisure activities so as to define their impact on life in society, as well as to racial and cultural integration and on socio-cultural changes in these areas. The concept of \"culture\" covers the sociology of science, religion, art, sport and leisure, and also comprises inter alia R&D activities on the media, the mastery of language and social integration, libraries, archives and external cultural policy. This objective also includes R&D activities related to: recreational and sporting services; cultural services; broadcasting or online streaming and publishing services; and religious and other community services."@en-ca ;
        crdc-ccrd:classTitle       "Culture, recreation, religion and media"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Division"@en-ca ;
        crdc-ccrd:hasID            "RDS110" ;
        crdc-ccrd:hasLevel         "1"^^xsd:double .

crdc-ccrd-data:RDS10605
        a                          owl:Class , skos:Concept , crdc-ccrd:CRDC_CCRD_Group ;
        rdfs:label                 "Agricultural and industrial chemicals"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 44" ;
        rdfs:subClassOf            crdc-ccrd-data:RDS106 ;
        crdc-ccrd:classDefinition  "This group covers research and experimental development (R&D) activities directed towards the manufacture of industrial chemicals and related products, including those derived from oil and gas refining; and, chemical fertilisers and chemicals for the protection of crops and livestock. It excludes veterinary pharmaceutical products."@en-ca ;
        crdc-ccrd:classTitle       "Agricultural and industrial chemicals"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Group"@en-ca ;
        crdc-ccrd:hasID            "RDS10605" ;
        crdc-ccrd:hasLevel         "2"^^xsd:double ;
        skos:broaderTransitive     crdc-ccrd-data:RDS106 .

crdc-ccrd-data:RDS108
        a                          owl:Class , crdc-ccrd:CRDC_CCRD_Division , skos:Concept ;
        rdfs:label                 "Agriculture (including fisheries and forestry)"@en-ca ;
        rdfs:seeAlso               " fileName: crdc-ccrd-2020-seo-ose-structure-eng row: 58" ;
        rdfs:subClassOf            crdc-ccrd:SEO_OSE_Entity ;
        crdc-ccrd:classDefinition  "This division is a socioeconomic objective which covers all research and experimental development (R&D) activities with objectives related to promote agriculture, forestry, fisheries and foodstuff production, or further knowledge on chemical fertilisers, biocides, biological pest control and the mechanisation of agriculture, as well as concerning the impact of agricultural and forestry activities on the environment. This objective also covers R&D activities aimed at improving food productivity and technology. It does not include R&D activities on the reduction of pollution (objective 2 or RDS102); on the development of rural areas; on the construction and planning of buildings; on the improvement of rural rest and recreation amenities and agricultural water supply (objective 4 or RDS104); on energy efficiency measures (objective 5 or RDS105); or on the food processing industry (objective 6 or RDS106)."@en-ca ;
        crdc-ccrd:classTitle       "Agriculture (including fisheries and forestry)"@en-ca ;
        crdc-ccrd:hasHierarchicalStructure
                "Division"@en-ca ;
        crdc-ccrd:hasID            "RDS108" ;
        crdc-ccrd:hasLevel         "1"^^xsd:double .

crdc-ccrd-data:RDS10505
